func ([aClass.getReceiverName()/] *[aClass.getMockName()/]_Prototype_Methods) [methodName/]([anEOperation.generateParameterList(imports,true)/]) [anEOperation.generateType(imports)/] {
	ret := [aClass.getReceiverName()/].mock.Called([anEOperation.generateParameterCall()/])

	var r [anEOperation.generateType(imports)/]
	if rf, ok := ret.Get(0).(func() [anEOperation.generateType(imports)/]); ok {
		r = rf()
	} else {
		if ret.Get(0) != nil {
			r = ret.Get(0)[if needCast].([anEOperation.generateType(imports)/])[/if]
		}
	}

	return r
}

