[template public generateMock(anEOperation : EOperation, aClass : EClass, imports : OclAny) ? (anEOperation.oclIsUndefined() or anEOperation.eGenericType.oclIsUndefined()){
	methodName : String = anEOperation.getOperationName();
	mockName : String = aClass.getMockName();
	mockExpecterName : String =mockName +'_Expecter_Methods';
	mockCallName : String = mockName+'_' +methodName+'_Call';
}]
// [methodName/] provides mock implementation
func ([aClass.getReceiverName()/] *[aClass.getMockName()/]_Prototype_Methods) [methodName/]([anEOperation.generateParameterList(imports,true)/]) {
	[aClass.getReceiverName()/].mock.Called([anEOperation.generateParameterCall()/])
}

type [mockCallName/] struct {
	*mock.Call
}

func (e *[mockExpecterName/]) [methodName/]([anEOperation.generateParameterList(imports,true)/]) *[mockCallName/] {
	return &[mockCallName/]{Call: e.mock.On("[methodName/]",[anEOperation.generateParameterCall()/])}
}

func (c *[mockCallName/]) Run(run func([anEOperation.generateParameterList(imports,false)/])) *[mockCallName/] {
	c.Call.Run(func(_args mock.Arguments) {
		run([for (eParameter : EParameter | anEOperation.eParameters) separator (',')][let needCast : Boolean = not(not eParameter.oclIsUndefined() and eParameter.upperBound = 1 and not eParameter.eGenericType.oclIsUndefined() and eParameter.eGenericType.eClassifier.isAny())]_args['['/][i-1/][']'/][if needCast].([eParameter.generateType(imports)/])[/if][/let][/for])
	})
	return c
}

func (c *[mockCallName/]) Return() *[mockCallName/] {
	c.Call.Return()
	return c
}
