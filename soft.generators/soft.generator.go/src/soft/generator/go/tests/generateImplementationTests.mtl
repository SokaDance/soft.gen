[query private isFeatureMap(aStructuralFeature : EStructuralFeature) : Boolean = isListType() and eType.name = 'EFeatureMapEntry'/]

[query private getMapTypes(aStructuralFeature : EStructuralFeature) : Set(EClassifier) =
	if isMapType() and eType.oclIsKindOf(EClass) and eType.oclAsType(EClass).isMapEntry() then
		let mapType : EClass = eType.oclAsType(EClass) in
		Set{ mapType.getEStructuralFeature('key').eType, mapType.getEStructuralFeature('value').eType }
	else
		Set{}
	endif
/]

[template public generateImplementationTests(aClass : EClass) { 
	implementationName : String = aClass.getImplementationName();
	implementedFeatures : Set(EStructuralFeature) = aClass.getImplementedFeatures();
	aPackage : EPackage = aClass.ePackage;
	requiresEcore : Boolean = not aPackage.isEcorePackage() and ( aClass.getImplementedFeatures()->notEmpty() or aClass.requiresList() );
}]
[file (aClass.ePackage.getOutputDirectory() + '/' + aClass.getImplementationTestFileName( aClass.name ), false, 'UTF-8')]
[fileComment()/]

package [aPackage.name/]



import (
	"testing"
	"github.com/stretchr/testify/assert"
	"github.com/stretchr/testify/mock"
[let imports : OrderedSet(String) = Set { if requiresEcore then Set{ aPackage.getImportPathForPackage('ecore') } else Set{} endif
										, OrderedSet{aClass.eSuperTypes
              									    ,implementedFeatures.eType
													,implementedFeatures->collect( getMapTypes() )->flatten()
													}->flatten()->select( not oclIsUndefined() )->collect( cls | aPackage.getImportPath(cls) )->asSet()
										, implementedFeatures->collect( f : EStructuralFeature | f.eType.getImportPathForTestValue(aPackage) )->asSet()
										}->flatten()->select( s | s <> '')->asOrderedSet()->sortedBy( s | s) ]
[for ( i : String | imports )]
	"[i/]"
[/for]
[/let]
)

func discard[name.toUpperFirst()/]() {
	_ = assert.Equal
	_ = mock.Anything
	_ = testing.Coverage
[if requiresEcore]
	_ = ecore.ADD
[/if]
}

